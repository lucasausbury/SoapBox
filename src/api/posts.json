---
layout: null
---
[{% for i in (0..100) %}{% for post in site.posts %}
    {
        "title":"{{ post.title }}",
        "excerpt":"{{ post.excerpt | strip_newlines }}",
        "tags":["{{ post.tags | join: '","' }}"],
        "category":"{{ post.category }}",
        "date":"{{ post.date }}",
        "pretty_date":"{{ post.date | date: '%a, %b %d, %Y' }}",
        "url":"{{ post.url }}"
    }{% unless forloop.last %},{% endunless %}
{% endfor %}{% unless forloop.last %},{% endunless %}{% endfor %}]